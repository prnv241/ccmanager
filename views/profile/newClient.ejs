 
<%- include("../partials/header.ejs") %>

<div id="cent">
  <div class="container boxes">
    <h2 class="mt-3" style="text-align: center;">Complete your profile</h2>
    <form action="/profile/client" method="POST" class="mt-5"> 
        <div class="form-group">
          <label>Name</label>
          <input type="text" class="form-control" name="prof[name]" required>
        </div>
        <div class="form-group">
          <label>Email</label>
          <input type="email" class="form-control" name="prof[email]" required>
        </div>
        <div class="form-group">
          <label>Mobile No.</label>
          <input type="number" class="form-control" name="prof[number]" required>
        </div>
        <div class="form-group">
          <label>Profile Picture URL</label>
          <input type="text" class="form-control" name="prof[propic]" required>
        </div>
        <div class="form-group mt-5">
            <button type="submit" class="btn btn-primary">Submit</button>
        </div>
    </form>
  </div>
</div>

<%- include("../partials/footer.ejs") %>